{
  "name": "blaise-editing-service",
  "version": "0.1.0",
  "private": true,
  "target": "esnext",
  "module": "esnext",
  "engines": {
    "node": ">=20.19.5"
  },
  "scripts": {
    "start": "yarn start-server",
    "build": "yarn build-client && yarn build-server",
    "clean": "npx rimraf dist",
    "test": "yarn clean && yarn build && yarn test-vitest",
    "test-vitest": "vitest --coverage --no-watch",
    "test:bail": "yarn test-vitest --bail 1",
    "test:watch": "vitest -watch",
    "test-fast": "yarn clean && yarn build && vitest --coverage --no-watch",
    "lint": "yarn eslint . --fix",
    "dev": "yarn clean && yarn build && yarn start-server",
    "dev-separate": "yarn build && concurrently npm:start-client npm:start-server",
    "gcp-build": "GENERATE_SOURCEMAP=false yarn build",
    "preview-build": "yarn build && vite preview",
    "build-client": "vite build",
    "start-client": "vite",
    "build-server": "tsc --project tsconfig.server.json",
    "start-server": "node ./dist/server/index.js"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "blaise-api-node-client": "git+https://github.com/ONSdigital/blaise-api-node-client#2.0.1",
    "blaise-design-system-react-components": "git+https://github.com/ONSdigital/blaise-design-system-react-components#1.0.3",
    "@babel/traverse": "^7.23.2",
    "form-data": "^2.5.4",
    "qs": "^6.14.1",
    "jws": "^4.0.0",
    "brace-expansion": "^2.0.2"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "axios": "^1.12.0",
    "blaise-api-node-client": "git+https://github.com/ONSdigital/blaise-api-node-client#2.0.1",
    "blaise-design-system-react-components": "git+https://github.com/ONSdigital/blaise-design-system-react-components#1.0.3",
    "blaise-login-react": "git+https://github.com/ONSdigital/blaise-login-react#1.1.5",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "express": "^4.21.2",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "web-vitals": "^2.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^8.0.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "^4.0.15",
    "axios-mock-adapter": "^1.21.5",
    "eslint": "^9.39.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "express-list-endpoints": "^6.0.0",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.0.0",
    "supertest": "^7.1.4",
    "typemoq": "^2.1.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.50.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.15"
  }
}
