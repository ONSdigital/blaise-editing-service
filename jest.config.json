{
  "preset": "ts-jest",
  "moduleNameMapper": {
    "\\.(css|less|scss)$": "identity-obj-proxy"
},
"setupFilesAfterEnv": [
  "<rootDir>/src/setupTests.ts"
], 
  "testEnvironment": "jsdom",
  "testEnvironmentOptions": {
    "html": "<html lang=\"zh-cmn-Hant\"></html>",
    "url": "https://jestjs.io/",
    "userAgent": "Agent/007"
  },
  "globals": {
    "IS_REACT_ACT_ENVIRONMENT": true
  }  
}
